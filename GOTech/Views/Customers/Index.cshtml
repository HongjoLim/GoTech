@model IEnumerable<GOTech.Models.ApplicationUser>

@{
    ViewBag.Title = "Our customers";
}
<div id="employeeHeader">
    @Html.Partial("_CustomerHeader")
</div>
<h2 class="display-4">@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            Sining up Date
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HiringDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { email = item.Email }) |
            @Html.ActionLink("Details", "Details", new { email = item.Email }) |
            @Html.ActionLink("Delete", "Delete", new { email = item.Email })
        </td>
    </tr>
}

</table>
